{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "monorepo": true,
  "root": "apps/api-gateway",
  "projects": {
    "accounts-service": {
      "type": "application",
      "root": "apps/accounts-service",
      "entryFile": "main",
      "sourceRoot": "apps/accounts-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/accounts-service/tsconfig.app.json"
      }
    },
    "api-gateway": {
      "type": "application",
      "root": "apps/api-gateway",
      "entryFile": "main",
      "sourceRoot": "apps/api-gateway/src",
      "compilerOptions": {
        "tsConfigPath": "apps/api-gateway/tsconfig.app.json"
      }
    },
    "auth": {
      "type": "library",
      "root": "libs/auth",
      "entryFile": "index",
      "sourceRoot": "libs/auth/src",
      "compilerOptions": {
        "tsConfigPath": "libs/auth/tsconfig.lib.json"
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    },
    "config": {
      "type": "library",
      "root": "libs/config",
      "entryFile": "index",
      "sourceRoot": "libs/config/src",
      "compilerOptions": {
        "tsConfigPath": "libs/config/tsconfig.lib.json"
      }
    },
    "database": {
      "type": "library",
      "root": "libs/database",
      "entryFile": "index",
      "sourceRoot": "libs/database/src",
      "compilerOptions": {
        "tsConfigPath": "libs/database/tsconfig.lib.json"
      }
    },
    "documents-service": {
      "type": "application",
      "root": "apps/documents-service",
      "entryFile": "main",
      "sourceRoot": "apps/documents-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/documents-service/tsconfig.app.json"
      }
    },
    "estate-service": {
      "type": "application",
      "root": "apps/estate-service",
      "entryFile": "main",
      "sourceRoot": "apps/estate-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/estate-service/tsconfig.app.json"
      }
    },
    "family-service": {
      "type": "application",
      "root": "apps/family-service",
      "entryFile": "main",
      "sourceRoot": "apps/family-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/family-service/tsconfig.app.json"
      }
    },
    "messaging": {
      "type": "library",
      "root": "libs/messaging",
      "entryFile": "index",
      "sourceRoot": "libs/messaging/src",
      "compilerOptions": {
        "tsConfigPath": "libs/messaging/tsconfig.lib.json"
      }
    },
    "notification": {
      "type": "library",
      "root": "libs/notification",
      "entryFile": "index",
      "sourceRoot": "libs/notification/src",
      "compilerOptions": {
        "tsConfigPath": "libs/notification/tsconfig.lib.json"
      }
    },
    "notifications-service": {
      "type": "application",
      "root": "apps/notifications-service",
      "entryFile": "main",
      "sourceRoot": "apps/notifications-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/notifications-service/tsconfig.app.json"
      }
    },
    "observability": {
      "type": "library",
      "root": "libs/observability",
      "entryFile": "index",
      "sourceRoot": "libs/observability/src",
      "compilerOptions": {
        "tsConfigPath": "libs/observability/tsconfig.lib.json"
      }
    },
    "succession-automation-service": {
      "type": "application",
      "root": "apps/succession-automation-service",
      "entryFile": "main",
      "sourceRoot": "apps/succession-automation-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/succession-automation-service/tsconfig.app.json"
      }
    }
  },
  "compilerOptions": {
    "webpack": true
  }
}