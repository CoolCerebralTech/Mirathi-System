{
  "name": "@shamba/documents-service",
  "version": "1.0.0",
  "description": "Documents service for Shamba Sure - Secure document management and verification",
  "private": true,
  "main": "dist/apps/documents-service/main.js",
  "types": "dist/apps/documents-service/main.d.ts",
  "scripts": {
    "build": "nest build documents-service",
    "start": "nest start documents-service",
    "start:dev": "nest start documents-service --watch",
    "start:debug": "nest start documents-service --debug --watch",
    "start:prod": "node dist/apps/documents-service/main.js",
    "test": "jest --config ../../jest.config.js --runInBand",
    "test:watch": "jest --config ../../jest.config.js --watch",
    "test:cov": "jest --config ../../jest.config.js --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register ../../node_modules/.bin/jest --runInBand",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf dist/apps/documents-service"
  },
  "dependencies": {
    "@nestjs/common": "^11.0.0",
    "@nestjs/core": "^11.0.0",
    "@nestjs/platform-express": "^11.0.0",
    "@nestjs/swagger": "^7.3.0",
    "@shamba/auth": "workspace:^1.0.0",
    "@shamba/common": "workspace:^1.0.0",
    "@shamba/config": "workspace:^1.0.0",
    "@shamba/database": "workspace:^1.0.0",
    "@shamba/messaging": "workspace:^1.0.0",
    "@shamba/observability": "workspace:^1.0.0",
    "axios": "^1.7.7",
    "class-transformer": "^0.5.1",
    "mime-types": "^2.1.35",
    "multer": "1.4.5-lts.2",
    "nestjs-pino": "^4.4.1",
    "pdf-parse": "^1.1.1",
    "pino-pretty": "^11.3.0",
    "sharp": "^0.33.4",
    "uuid": "^11.0.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.0",
    "@nestjs/testing": "^11.0.0",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/mime-types": "^2.1.4",
    "@types/multer": "^1.4.13",
    "@types/node": "^20.11.30",
    "@types/pdf-parse": "^1.1.3",
    "@types/sharp": "^0.32.0",
    "@types/uuid": "^11.0.0",
    "eslint": "^9.10.0",
    "jest": "^29.7.0",
    "rimraf": "^6.0.1",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.6.3"
  }
}
